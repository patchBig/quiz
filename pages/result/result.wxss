/**result.wxss**/
.container {
  padding: 40rpx;
  text-align: center;
}
.title {
  font-size: 48rpx;
  font-weight: bold;
  margin-bottom: 40rpx;
}
.score {
  font-size: 36rpx;
  margin-bottom: 20rpx;
}
.message {
  font-size: 32rpx;
  color: #666666;
  margin-bottom: 60rpx;
}
.btn-group {
  display: flex;
  justify-content: space-around;
}
.btn {
  background-color: #1aad19;
  color: #ffffff;
  padding: 20rpx 40rpx;
  border-radius: 10rpx;
  font-size: 28rpx;
}
